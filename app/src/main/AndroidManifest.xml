<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TestNineActivity">
        </activity>
        <activity android:name=".PkActivity">
        </activity>
        <activity android:name=".TestRecyclerViewActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.aac.TestAACActivity">
        </activity>
        <activity android:name="com.example.administrator.rooms.RoomActivity">
        </activity>
        <activity android:name="com.example.administrator.AudioRhythm.AudioActivity">
        </activity>
        <activity android:name="com.example.administrator.body.TestBodyActivity">
        </activity>
        <activity android:name=".RecyclerViewDragActivity">
        </activity>
        <activity android:name="com.example.administrator.layoutopt.LayoutOptActivity">
        </activity>
        <activity android:name="com.example.administrator.animate.AnimateActivity">
        </activity>
        <activity android:name="com.example.administrator.font.FontActivity">
        </activity>
        <activity android:name="com.example.administrator.drawable.DrawableActivity">
        </activity>
        <activity
            android:name="com.example.administrator.spannable.SpannableActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name="com.example.administrator.marquee.MarqueeActivity">
        </activity>
    </application>

</manifest>