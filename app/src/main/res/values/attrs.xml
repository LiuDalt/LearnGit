<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RingView">
        <attr name="radius" format="dimension" ></attr>
        <attr name="stroke_width" format="dimension"></attr>
        <attr name="ring_color" format="color"></attr>
        <attr name="arc_type" format="enum">
            <enum name="quarter" value="0"></enum>
            <enum name="two_quarter" value="1"></enum>
            <enum name="three_quarter" value="2"></enum>
            <enum name="circle" value="3"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="LiveMarqueeTextView">
        <attr name="marqueeTimesOnCreate" format="integer" ></attr>
        <attr name="text" format="string"></attr>
        <attr name="marqueeTextSize" format="dimension"></attr>
        <attr name="marqueeTextColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="LiveDailyRankView">
        <attr name="normalTextSize" format="dimension"></attr>
        <attr name="rankMarqueeTextColor" format="color"></attr>
        <attr name="normalTextColor" format="color"></attr>
    </declare-styleable>
</resources>